#:import option_bar option_bar

<OptionBar>:
    orientation: 'horizontal'
    size_hint_y: None
    height: 40

    Button:
        text: "Load atlased character"
        on_release: root.open_atlased_character_loader()

    Button:
        text: "Load new character"
        on_release: root.open_png_character_loader()